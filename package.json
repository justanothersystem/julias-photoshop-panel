{
  "name": "julias-photoshop-panel",
  "version": "1.0.0",
  "description": "A web app and Photoshop panel that allows audience members to upload expiring images for Julia to work with.",
  "private": true,
  "now": {
    "dotenv": ".env.now",
    "alias": ["jp.justanothersystem.org"]
  },
  "scripts": {
    "start": "NODE_ENV=production node jp.justanothersystem.org/index.js",
    "build": "NODE_ENV=production webpack --config jp.justanothersystem.org/webpack.config.babel.js --define process.env.NODE_ENV=\"production\" --progress --colors",
    "dev": "NODE_ENV=development concurrently \"webpack-dev-server --config jp.justanothersystem.org/webpack.config.babel.js\" \"npm run watch\" \"npm run browser-sync\" --names \"WDS,NRW,BSY\" --prefix name",
    "watch": "nodemon ./jp.justanothersystem.org/index.js --ignore jp.justanothersystem.org/public/",
    "browser-sync": "browser-sync start --no-open --proxy \"localhost:9000\" --no-notify --files \"jp.justanothersystem.org/views/*.pug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justanothersystem/julias-photoshop-panel.git"
  },
  "author": "Jeffrey Alan Scudder <me@jas.life>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/justanothersystem/julias-photoshop-panel/issues"
  },
  "homepage": "https://github.com/justanothersystem/julias-photoshop-panel#readme",
  "dependencies": {
    "babel-register": "^6.26.0",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
