<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ExtensionBundleId="com.adobe.CEPHTMLTEST" ExtensionBundleVersion="1.0" Version="6.0"> <!-- MAJOR-VERSION-UPDATE-MARKER -->
    <ExtensionList>
        <Extension Id="org.justanothersystem.julias-panel" Version="1.0.0"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="PHSP" Version="16.0"/>
            <Host Name="PHXS" Version="16.0"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="6.0"/> <!-- MAJOR-VERSION-UPDATE-MARKER -->
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="org.justanothersystem.julias-panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./html/index.html</MainPath>
					<ScriptPath>./jsx/core.jsx</ScriptPath>
                    <CEFCommandLine>
						<Parameter>--nodejs-disabled</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Julia's Panel</Menu>
                    <Geometry>
                        <Size>
                            <Height>320</Height>
                            <Width>240</Width>
                        </Size>
                        <MaxSize>
                            <Height>640</Height>
                            <Width>480</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>160</Height>
                            <Width>120</Width>
                        </MinSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./light.png</Icon>
                        <Icon Type="DarkNormal">./dark.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
